{
  "name": "Does it have my keywords?",
  "version": "1.3",
  "description": "Times how long it takes to send a message to a content script and back.",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["thirdparty/jquery-3.1.1.min.js", "page.js"]
    }
  ],
  "background" : {
    "scripts" : ["page.js"]
  },
	"permissions": [
          "storage",
          "activeTab"
  ],
  "browser_action": {
    "default_title": "Time to current page",
    "default_icon": "clock.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
